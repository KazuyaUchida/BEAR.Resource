{
    "name": "BEAR/Resource",
    "description": "RESTful service layer framework",
    "keywords": ["rest", "service layer"],
    "homepage": "https://github.com/koriym/BEAR.Resource",
    "version": "0.4.0",
    "license": "BSD-3-Clause",
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "facebook/xhprof",
                "version": "0.9.2",
                "dist": {
                    "url": "https://github.com/facebook/xhprof/zipball/master",
                    "type": "zip"
                },
                "source": {
                    "url": "https://github.com/facebook/xhprof.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "aura/di": "dev-master",
        "aura/signal": "dev-master",
        "doctrine/common": "2.3.*",
        "guzzle/guzzle": "2.7.*",
        "ray/aop": ">=1.0.0",
        "ray/di": ">=1.0.0",
        "facebook/xhprof": "0.9.2"
    },
    "authors": [
        {
            "name": "Akihito Koriyama",
            "email": "akihito.koriyama@gmail.com"
        }
    ],
    "autoload": {
        "psr-0": {
            "BEAR\\Resource": "src/",
            "Aura\\Di": "vendor/Aura/Di/src/",
            "Aura\\Signal": "vendor/Aura/Signal/src/"
        }
    }
}
